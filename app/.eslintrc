{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:meteor/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "prettier",
    "airbnb"
  ],
  "settings": {
    "import/resolver": "meteor"
  },
  "plugins": ["meteor", "import", "prettier", "i18n"],
  "rules": { "no-underscore-dangle": "off" },
  "env": {
    "browser": true,
    "jest": true,
    "meteor": true,
    "es6": true,
    "node": true
  }
}
